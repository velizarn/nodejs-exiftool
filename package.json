{
  "name": "nodejs-exiftool",
  "version": "0.0.1",
  "description": "Node.js integration for Phil Harvey's ExifTool",
  "author": {
    "name": "Velizar Nenov",
    "url": "https://velizarn.github.io/"
  },
  "engines": {
    "node": "10.17.0"
  },
  "main": "index.js",
  "scripts": {
    "heroku-prebuild": "echo This runs before Heroku installs your dependencies.",
    "heroku-postbuild": "echo This runs afterwards.",
    "lint": "eslint --ext .js,.json,.jsx . --fix",
    "start": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 src/index.js",
    "start:dev": "nodemon --optimize_for_size --max_old_space_size=920 --gc_interval=100 src/index.js",
    "test": "cross-env NODE_ENV=test LOG_LEVEL=error mocha --timeout 10000 --exit",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "babel-eslint": "10.0.3",
    "body-parser": "1.19.0",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "chai-json": "1.0.0",
    "cron": "1.7.2",
    "dotenv": "8.2.0",
    "ejs": "2.7.4",
    "eslint-plugin-json": "2.0.1",
    "node-exiftool": "2.3.0",
    "express": "4.17.1",
    "express-validator": "6.3.0",
    "helmet": "3.21.2",
    "heroku-logger": "0.3.3",
    "http-auth": "3.2.4",
    "mocha": "6.2.2",
    "sqreen": "1.37.1",
    "throng": "4.0.0",
    "snyk": "^1.260.0"
  },
  "devDependencies": {
    "cross-env": "6.0.3",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-security": "1.4.0",
    "nodemon": "2.0.1",
    "prettier": "1.19.1",
    "request": "2.88.0",
    "request-promise": "4.2.5",
    "tape": "4.11.0"
  },
  "keywords": [
    "node",
    "heroku-pipeline",
    "heroku",
    "exiftool",
    "exif"
  ],
  "nodemonConfig": {
    "ext": "js,json",
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "test/*"
    ],
    "verbose": true
  },
  "snyk": true
}
