{
  "name": "nodejs-exiftool",
  "version": "0.0.1",
  "description": "Node.js integration for Phil Harvey's ExifTool",
  "author": {
    "name": "Velizar Nenov",
    "url": "https://velizarn.github.io/"
  },
  "engines": {
    "node": "10.15.3"
  },
  "main": "index.js",
  "scripts": {
    "heroku-prebuild": "echo This runs before Heroku installs your dependencies.",
    "heroku-postbuild": "echo This runs afterwards.",
    "lint": "eslint --ext .js,.json,.jsx . --fix",
    "start": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 src/index.js",
    "start:dev": "nodemon --optimize_for_size --max_old_space_size=920 --gc_interval=100 src/index.js",
    "test": "cross-env NODE_ENV=test LOG_LEVEL=error mocha"
  },
  "dependencies": {
    "babel-eslint": "10.0.1",
    "body-parser": "1.18.3",
    "chai": "4.2.0",
    "chai-http": "4.2.1",
    "chai-json": "1.0.0",
    "cron": "1.7.0",
    "dotenv": "7.0.0",
    "ejs": "2.6.1",
    "eslint-plugin-json": "1.4.0",
    "node-exiftool": "2.3.0",
    "express": "4.16.4",
    "express-validator": "5.3.1",
    "helmet": "3.16.0",
    "heroku-logger": "0.3.3",
    "http-auth": "3.2.3",
    "mocha": "6.1.4",
    "sqreen": "1.30.1",
    "throng": "4.0.0"
  },
  "devDependencies": {
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-security": "1.4.0",
    "nodemon": "1.18.11",
    "prettier": "1.17.0",
    "request": "2.88.0",
    "request-promise": "4.2.4",
    "tape": "4.10.1"
  },
  "keywords": [
    "node",
    "heroku-pipeline",
    "heroku",
    "exiftool",
    "exif"
  ],
  "nodemonConfig": {
    "ext": "js,json",
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "test/*"
    ],
    "verbose": true
  }
}
